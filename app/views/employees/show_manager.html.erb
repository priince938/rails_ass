<h1>Managers</h1>
<table class="table table-bordered table-hover">
  <thead class="thead-dark"> 
    <tr>
      <th>Manager</th>
      <th>City</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @managers.each do |manager| %>
      <tr>
        <td><%= Employee.find(manager.manager_id).name %></td>
        <td><%= City.find(Employee.find(manager.manager_id).city_id).name %></td>
       
        
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @managers ,renderer: WillPaginate::ActionView::BootstrapLinkRenderer%>
